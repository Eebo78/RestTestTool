<Window x:Class="Epim.RestTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:Epim.RestTest.Views" 
        Title="MainWindow" WindowStartupLocation="CenterScreen" WindowState="Maximized" MinHeight="800" MinWidth="1200"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
    <Grid>
        <TabControl>
            <TabItem Header="Performance">
                <views:PerformanceView />
            </TabItem>
            <TabItem Header="Test data">
                <views:AddTestDataView />
            </TabItem>
        </TabControl>
        
    </Grid>
    
</Window>
